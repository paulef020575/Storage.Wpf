<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Storage.Wpf">
    <DataTemplate DataType="{x:Type local:TypeProdViewModel}">
        <local:TypeProdView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:RefTypeProdViewModel}">
        <local:ReferenceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:SpeciesViewModel}">
        <local:SpeciesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:RefSpeciesViewModel}">
        <local:ReferenceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:GradeViewModel}">
        <local:GradeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:RefGradeViewModel}">
        <local:ReferenceView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:QuestionViewModel}">
        <local:QuestionView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:StoreViewModel}">
        <local:StoreView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:RefStoreViewModel}">
        <local:ReferenceView />
    </DataTemplate>
</ResourceDictionary>