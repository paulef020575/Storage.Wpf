<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Storage.Wpf.Resources"
                    xmlns:wpf="clr-namespace:Storage.Wpf">
    <DataTemplate DataType="{x:Type wpf:NomenclatureViewModel}">
        <wpf:NomenclatureView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:GradeViewModel}">
        <wpf:GradeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:SpeciesViewModel}">
        <wpf:SpeciesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:StoreViewModel}">
        <wpf:StoreView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:TypeProdViewModel}">
        <wpf:TypeProdView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:CustomerViewModel}">
        <wpf:CustomerView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:RoadViewModel}">
        <wpf:RoadView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type wpf:StoreCellViewModel}">
        <wpf:StoreCellView />
    </DataTemplate>
</ResourceDictionary>